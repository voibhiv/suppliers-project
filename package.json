{
  "name": "suppliers_project",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Leandro Melo <leo1309@edu.uniube.br>",
  "license": "MIT",
  "scripts": {
    "build": "tsc --project ./",
    "start:dev": "ts-node-dev --transpile-only src/server.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:generate ./src/database/migrations/default",
		"migration:run": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:run",
    "start:prod": "ts-node dist/server.js",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "style:check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "style:fix": "prettier --write 'src/**/*.ts' 'test/**/*.ts'"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "module-alias": "^2.2.2",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.3",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.15.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.2"
  }
}
